<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_history"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/back"
        android:layout_height="@dimen/back"
        android:layout_marginStart="@dimen/padding_back"
        android:layout_marginTop="@dimen/padding_back"
        android:layout_marginBottom="@dimen/back"
        tools:srcCompat="@tools:sample/avatars"
        android:contentDescription="@string/history" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center_horizontal">

        <TextView
            android:id="@+id/textHistory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/history" />

        <ListView
            android:id="@+id/listView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></ListView>

    </LinearLayout>

</LinearLayout>